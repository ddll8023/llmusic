{
	"name": "llmusic",
	"productName": "LLMusic",
	"version": "1.0.0",
	"description": "",
	"main": "src-main/main.js",
	"scripts": {
		"dev": "concurrently \"npm run dev1\" \"npm run dev2\"",
		"dev2": "electron .",
		"dev1": "vite --port 5173",
		"build": "vite build",
		"pack": "electron-builder",
		"preview": "vite preview"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"ffmpeg-static": "^5.2.0",
		"ffprobe-static": "^3.1.0",
		"fluent-ffmpeg": "^2.1.3",
		"lowdb": "^7.0.1",
		"music-metadata": "^11.2.3",
		"pinia": "^3.0.3",
		"uuid": "^11.1.0",
		"vue": "^3.5.16",
		"vue-virtual-scroller": "^2.0.0-beta.8"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^5.2.4",
		"concurrently": "^8.2.2",
		"electron": "^36.5.0",
		"electron-builder": "^26.0.12",
		"element-plus": "^2.10.5",
		"font-awesome": "^4.7.0",
		"sass": "^1.89.2",
		"vite": "^6.3.5"
	},
	"build": {
		"appId": "com.electron.llmusic",
		"productName": "LLMusic",
		"asarUnpack": [
			"**/node_modules/ffmpeg-static/**",
			"**/node_modules/ffprobe-static/**"
		],
		"fileAssociations": [
			{
				"ext": "mp3",
				"name": "MP3 Audio File",
				"description": "MP3 音频文件",
				"role": "Editor"
			},
			{
				"ext": "flac",
				"name": "FLAC Audio File",
				"description": "FLAC 无损音频文件",
				"role": "Editor"
			},
			{
				"ext": "wav",
				"name": "WAV Audio File",
				"description": "WAV 音频文件",
				"role": "Editor"
			},
			{
				"ext": "m4a",
				"name": "M4A Audio File",
				"description": "M4A 音频文件",
				"role": "Editor"
			},
			{
				"ext": "ogg",
				"name": "OGG Audio File",
				"description": "OGG 音频文件",
				"role": "Editor"
			},
			{
				"ext": "aac",
				"name": "AAC Audio File",
				"description": "AAC 音频文件",
				"role": "Editor"
			}
		]
	}
}
